{
  "manifest_version": 2,
  "name": "WrikeDarkmode",
  "version": "1.0",
  "description": "Darkmode for Wrike",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://app-eu.wrike.com/*"],
      "js": ["content.js"]
    }
  ]
}
